{
    "name": "decodelabs/glitch",
    "description": "Better tools for when things go wrong",
    "type": "library",
    "keywords": [ "dump", "dumper", "exception", "debug" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",
        "symfony/polyfill-mbstring": "^1.33",

        "decodelabs/coercion": "^0.3.5",
        "decodelabs/enlighten": "^0.9.4",
        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/kingdom": "^0.1",
        "decodelabs/monarch": "^0.2",
        "decodelabs/nuance": "^0.1.9",
        "decodelabs/remnant": "^0.1.2",

        "psr/log": "^3.0.2"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7",

        "symfony/var-dumper": "^5|^6|^7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Glitch\\": "src/Glitch"
        },
        "classmap": [
            "src/Glitch.php"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.21.x-dev"
        }
    },
    "repositories": {
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
